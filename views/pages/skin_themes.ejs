<!DOCTYPE html>
<html lang="en">

  <head>
    <%- include('../partials/head.ejs') %>
      <title>Skin Themes List</title>
  </head>

  <body>
    <div class="bg-image"></div>
    <div class="container">
      <h1 class="text-center gallery-title">Skin Theme Gallery</h1>
      <div class="row">
        <% skinThemes.forEach(skinType=> { %>
          <%- include('../partials/cards/base.ejs', {
            className: 'skin-card',
            data_attr_name: 'skin-name',
            data_attr_val: skinType.name,
            displayIcon: skinType.image,
            displayName: skinType.name
          }) %>
        <% }); %>
      </div>
    </div>
    <script src="/js/bootstrap.bundle.min.js"></script>
    <script src="/js/jquery-3.7.1.min.js"></script>
    <script src="/js/main.js"></script>
  </body>

</html>
<!DOCTYPE html>
<html lang="en">

  <head>
    <%- include('../partials/head.ejs') %>
      <title>Skin Themes List</title>
  </head>

  <body>
    <div class="bg-image"></div>
    <div class="container">
      <h1 class="text-center gallery-title">Skin Theme Gallery</h1>

      <%- include('../partials/topbar.ejs', { search_url: current_path }) %>

      <div class="row">
        <% skinThemes.forEach(skinType=> { %>
          <%- include('../partials/cards/base.ejs', {
            link: `/skins?skin_theme=${skinType.name}`,
            displayIcon: skinType.image,
            displayName: skinType.name
          }) %>
        <% }); %>
      </div>
    </div>
    <script src="/js/bootstrap.bundle.min.js"></script>
    <script src="/js/jquery-3.7.1.min.js"></script>
    <script src="/js/main.js"></script>
  </body>

</html>